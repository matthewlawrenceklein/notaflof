
<div class="centering_div">
    <div class="venue_info">
        <h1>Venue Page</h1>
        <div>
            <%= button_to 'Add Venue', new_venue_path, method: :get %>
        </div>
        <% @venues.each do |venue|%>
            <ul>
                <li>
                    <%= link_to "#{venue.name}, zipcode: #{venue.zipcode}", venue_path(venue) %>
                </li>
            </ul>
        <% end %>
    </div>
</div>